<!-- 查询角色树   模糊查询角色 -->
	<select id="queryLikeRoleTreeList" resultType="byit.aladdin.report.bo.IdaPmRoleBo" parameterType="java.util.Map">
		select distinct r.id, r.name from auth_role r left join rep_menu_role_rela rr  on rr.roleid = r.id  where r.STATE = '1'
		
		      <if test= "id != null and id != '' ">
		        and id = #{id,jdbcType=VARCHAR}     
		      </if>
		      <if test= "name != null and name != '' ">
		       <!--  and name like CONCAT('%',#{name,jdbcType=VARCHAR},'%')   -->
		       and name like  CONCAT(CONCAT('%',#{name,jdbcType=VARCHAR}),'%')   
		      </if>
		      <if test= "menuparentId != null and menuparentId != '' ">
		        and menuid = #{menuparentId,jdbcType=VARCHAR}      
		      </if>
	     
	      order by r.id
	</select>
	
