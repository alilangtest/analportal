<!-- 查询列表 -->
	<select id="queryReprotPage" parameterType="java.util.Map" resultType="byit.aladdin.report.entity.IdaPmReport">
		select r.reportid,r.projectname,r.workbookname,r.reportname,r.reporttype,
		r.publisper,r.reportdescription,r.desp,r.dateScreen,r.publishdate,r.mountstate from PM_REPORT r where 1=1
		<!-- select r.*,nvl2(a.eventid,'已挂载','未挂载') MOUNTSTATE from PM_REPORT r left join rep_menu_rela a on r.reportid=a.reportid where 1=1 -->
		<if test="reportname!=null and reportname!=''">
		         and r.reportname LIKE CONCAT(CONCAT('%',#{reportname,jdbcType=VARCHAR}),'%')
		</if>
		<if test="menuid!=null and menuid!='' and flag==''">
			and r.reportid in(select reportid from REP_MENU_RELA rm where rm.Menuid=#{menuid}) 
		</if>
		<if test="menuid!=null and menuid!='' and flag!='' and flag!=null ">
			and r.reportid not in(select reportid from REP_MENU_RELA rm) 
		</if>
		<if test="mountstate !=null and mountstate !=''">
			and r.mountstate = #{mountstate,jdbcType=VARCHAR}
		</if>
		<!-- order by a.eventid desc ,r.updatedate desc -->
		order by r.gettime desc, r.mountstate asc, r.updatedate desc
	</select>
	
	
<!-- 查询 -->
	<select id="getReportById" resultType="byit.aladdin.report.bo.IdaPmReportBo" parameterType="byit.aladdin.report.bo.IdaPmReportBo">
