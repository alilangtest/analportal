#mycard-plus h4{
	height: 20px;
    margin: 0;
    line-height: 20px;
    text-align: left;
    font-size: 15px;
    margin-bottom: 10px;
}
/* tag */
.add_box_in_ul .add_box_in_uli .default-tag a,.default-tag a span,.plus-tag a,.plus-tag a em,.plus-tag-add a{background:url(../images/tagbg.png) no-repeat;}
.add_box_in_ul .add_box_in_uli .tagbtn a{color:#333333;display:block;float:left;height:22px;line-height:22px;overflow:hidden;margin:0 10px 10px 0;padding:0 10px 0 5px;white-space:nowrap;}
/* default-tag */
.add_box_in_ul .add_box_in_uli .default-tag{padding:0 0 0 0;}
.add_box_in_ul .add_box_in_uli .default-tag a{background-position:100% 0;}
.add_box_in_ul .add_box_in_uli .default-tag a:hover{background-position:100% -22px;}
.add_box_in_ul .add_box_in_uli .default-tag a span{padding:0 0 0 11px;background-position:0 -60px;width:auto;height:22px;line-height:22px;}
.add_box_in_ul .add_box_in_uli .default-tag a:hover span{background-position:0 -98px;}
.add_box_in_ul .add_box_in_uli .default-tag a.selected{opacity:0.6;filter:alpha(opacity=60);}
.add_box_in_ul .add_box_in_uli .default-tag a.selected:hover{background-position:100% 0;cursor:default;}
.add_box_in_ul .add_box_in_uli .default-tag a.selected:hover span{background-position:0 -60px;}
/* plus-tag */
.add_box_in_ul .add_box_in_uli .plus-tag{padding:0 0 0 0;}
.add_box_in_ul .add_box_in_uli .plus-tag a{background-position:100% -22px;}
.add_box_in_ul .add_box_in_uli .plus-tag a span{float:left;height:22px;line-height:22px;width:auto;}
.add_box_in_ul .add_box_in_uli .plus-tag a em{display:block;float:left;margin:5px 0 0 8px;width:13px;height:13px;overflow:hidden;background-position:-165px -100px;cursor:pointer;}
.add_box_in_ul .add_box_in_uli .plus-tag a:hover em{background-position:-168px -64px;}
/* plus-tag-add */
.add_box_in_ul .add_box_in_uli .plus-tag-add li{height:56px;position:relative;}
.add_box_in_ul .add_box_in_uli .plus-tag-add li .label{position:absolute;left:-110px;top:7px;display:block;}
.add_box_in_ul .add_box_in_uli .plus-tag-add button{float:left;}
.add_box_in_ul .add_box_in_uli .plus-tag-add a{float:left;margin:12px 0 0 20px;display:inline;font-size:18px;background-position:-289px -59px;padding:0 0 0 16px;}
.add_box_in_ul .add_box_in_uli .plus-tag-add a.plus{background-position:-289px -96px;}

